		section.registered
			.container
				h3 Registered devices
				
				.table-search
					.navigation
						.search-holder
							form(action="")
								label(for="") Search device
								.search-wrap
									input(type="text", placeholder="Serial No, Name")
									a(href="")
						.selects-holder
							.inline-layout
								.select-holder
									select#sel(name="")
										option(value="All", selected) All
										option(value="Stayin") Stay in Fence
										option(value="Dono") Do Nothing
										option(value="Dnm") Don`t Move
										option(value="TrH") Track History
								.select-holder
									select#sel1(name="")
										option(value="All", selected) All
										option(value="Red") Red
										option(value="Green") Green
										option(value="Gray") Gray
										option(value="Red-low") Red-low
										option(value="Green-low") Green-low
										option(value="Gray-low") Gray-low
				.scroll-holder-lg
					.devices-table
						table
							tbody
								tr
									td Serial No
									td.name
										span Name
									td
									td.desc Description
									td.plan
										span Asset Plan
										
									td.status
										span Alarm Status
										
								mixin newDeviceTableItem(href, serial, name, description, src, plan, status)
